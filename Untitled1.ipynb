   "source": [
    "from euler import *\n",
    "\n",
    "@memoize\n",
    "def factorial(n):\n",
    "    print 'calc:', n\n",
    "    if (n==0) | (n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)"
   ]
